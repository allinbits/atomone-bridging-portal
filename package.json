{
  "name": "@atomone/atomone-bridge",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.20.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test:e2e:serve": "serve -s dist -l 8080",
    "test:e2e": "npx playwright install && playwright test",
    "test:components": "npx playwright install && playwright test -c playwright-ct.config.ts",
    "test:components:debug": "npx playwright install && playwright test -c playwright-ct.config.ts --debug",
    "test:unit": "vitest run --reporter=verbose",
    "test:bridge": "vitest run --config vitest.e2e.config.ts --reporter=verbose",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@cosmjs/proto-signing": "^0.38.1",
    "@cosmostation/cosmos-client": "^0.0.5",
    "@safe-global/safe-apps-sdk": "^9.1.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/vue-query": "^5.92.5",
    "@types/markdown-it": "^14.1.2",
    "@unionlabs/sdk": "^2.0.0-beta.2",
    "@unionlabs/sdk-cosmos": "0.0.0-beta.0",
    "@unionlabs/sdk-evm": "0.0.4",
    "@vitest/coverage-istanbul": "^4.0.16",
    "@vueuse/core": "^14.1.0",
    "bech32": "^2.0.0",
    "bignumber.js": "^9.3.1",
    "cosmjs-types": "^0.11.0",
    "dayjs": "^1.11.19",
    "dompurify": "^3.3.1",
    "effect": "^3.19.14",
    "markdown-it": "^14.1.0",
    "markdown-it-github-alert": "^1.0.6",
    "viem": "^2.44.4",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vue": "^3.5.26",
    "vue-i18n": "^11.2.8",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@atomone/atomone-types": "^1.0.3",
    "@cosmjs/stargate": "^0.38.1",
    "@cosmjs/tendermint-rpc": "^0.38.1",
    "@keplr-wallet/types": "^0.12.308",
    "@playwright/experimental-ct-vue": "1.57.0",
    "@playwright/test": "1.57.0",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@tsconfig/node22": "^22.0.5",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^25.0.3",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/eslint-plugin": "^1.6.5",
    "@vitest/ui": "^4.0.16",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.23",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-plugin-playwright": "^2.4.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "~10.6.2",
    "happy-dom": "^20.0.11",
    "jiti": "^2.6.1",
    "jsdom": "^27.4.0",
    "mitt": "^3.0.1",
    "mobx": "^6.15.0",
    "mobx-vue-lite": "^0.4.3",
